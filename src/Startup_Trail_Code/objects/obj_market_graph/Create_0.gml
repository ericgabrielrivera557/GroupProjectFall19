if (obj_company.industry == industry_enum.FUSION)
{
	color1 = $71a6fb;
	color2 = $6c2e3c;
}
else if (obj_company.industry == industry_enum.GENETIC_ENG)
{
	color1 = $c5b9c9;
	color2 = $5f3040;
}
else if (obj_company.industry == industry_enum.QUANTUM_COMPUTING)
{
	color1 = $bd9431;
	color2 = $24200a;
}